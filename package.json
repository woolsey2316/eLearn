{
  "name": "elean",
  "version": "0.1.0",
  "description": "A starting point for RESTful MERN apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanhuiyang/TypeScript-MERN-Starter.git"
  },
  "author": "David Woolsey",
  "license": "MIT",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build-server": "yarn build-ts && yarn lint",
    "build": "cd client && yarn build && cd .. && yarn build-server",
    "build-ts": "tsc",
    "lint": "eslint -c .eslintrc.js --ext [js,ts,tsx]"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@types/react-router": "^5.1.16",
    "@types/react-router-redux": "^5.0.19",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "7.11.0",
    "eslint-plugin-jsdoc": "^39.4.0",
    "eslint-plugin-no-null": "^1.0.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.7.1",
    "ts-jest": "^26.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "bugs": {
    "url": "https://github.com/shanhuiyang/TypeScript-MERN-Starter/issues"
  },
  "homepage": "https://github.com/shanhuiyang/TypeScript-MERN-Starter#readme",
  "main": "server.js"
}
